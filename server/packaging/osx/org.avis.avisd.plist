<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
	"http://www.apple.
	com/DTDs/PropertyList-1.0.dtd">
	<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>org.avis.avisd</string>
		<key>Disabled</key>
		<false/>
		<key>OnDemand</key>
		<false/>
		<key>RunAtLoad</key>
		<true/>
		<key>ProgramArguments</key>
		<array>
			<string>/usr/local/sbin/avisd</string>
			<string>-logfile</string>
			<string>/var/log/avisd.log</string>
			<string>-c</string>
			<string>/usr/local/etc/avis/avisd.config</string>
		</array>
	</dict>
	</plist>
	